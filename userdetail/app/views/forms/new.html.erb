<%=form_for :form do |f| %>
<p>
	<%= f.label :firstname %><br>
	<%= f.text_field :firstname %>
</p>
<p>
	<%= f.label :lastname %><br>
	<%= f.text_area :lastname %>
</p>
<p>
	<%= f.label :email %><br>
	<%= f.text_area :email %>
</p>
<p>
	<%= f.label :password %><br>
	<%= f.text_area :password %>
</p>
<p>
	<%= f.submit %>
</p>
<%end%>