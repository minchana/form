
<h1>Editing details</h1>
<%= form_for :form, url: form_edit_path(@form), method: :put do |f| %>
	<p>
	<%= f.label :firstname %><br>
	<%= f.text_field :firstname %>
    </p>
    <p>
	<%= f.label :lastname %><br>
	<%= f.text_area :lastname %>
    </p>
    <p>
	<%= f.label :email %><br>
	<%= f.text_area :email %>
    </p>
    <p>
	<%= f.label :password %><br>
	<%= f.text_area :password %>
    </p>
    <p>
	<%= f.submit %>
    </p>
<%end%>